pipeline
  {
  
    agent any
	  stages
	  
	    {
		    stage ('scm checkout')
			 {
			   steps
			   
			   {
			   
			       git branch: 'master', url: 'https://github.com/kushgu06/maven-project.git'
			   
			   }
			 
			 
			 
			 }
		
		
	         stage ('sonar')
				{
				  steps 
				    {
					
					withSonarQubeEnv( 'mysonar') {
                                                 withMaven(jdk: 'localjdk', maven: 'localmaven') {
                                                                   sh 'mvn clean install sonar:sonar'
					
					
					}
				
				}
				
				}
				
				
				}
		     
		
		
		
		
		
		
		}
  
  
  }
  
  
  
  
  
  
  

