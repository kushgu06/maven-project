pipeline
  {
  
    agent any
	  stages
	  
	    {
		    stage
			 {
			   step ('scm checkout')
			   
			   {
			   
			       git branch: 'master', url: 'https://github.com/kushgu06/maven-project.git'
			   
			   }
			 
			 
			 
			 }
		
		
	         stage
				{
				  step  
				    {
					
					withSonarQubeEnv('sonar') {
                    withMaven(maven : 'LocalMaven') {
                    sh 'mvn clean package sonar:sonar'
					
					
					}
				
				}
				
				}
				
				
				}
		     
		
		
		
		
		
		
		}
  
  
  }
  
  
  
  
  
  
  
